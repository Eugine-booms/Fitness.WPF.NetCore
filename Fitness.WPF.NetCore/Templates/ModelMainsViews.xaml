<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Fitness.WPF.NetCore.ViewModel.UCViewModel"
                    xmlns:viewUC="clr-namespace:Fitness.WPF.NetCore.View.UserControls">





    <DataTemplate DataType="{x:Type vm:CreateNewUCVM}">
        <viewUC:CreateNewUserUC DataContext="{Binding }"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ChangeUserUCVM}">
        <viewUC:ChangeUserUC DataContext="{Binding}"/>
    </DataTemplate>

   

</ResourceDictionary>